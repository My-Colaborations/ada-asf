with "util";
with "util_config";
with "el";
with "security";
with "servlet_core";

library project Asf is
   Version := "@ASF_VERSION@";

   for Library_Name use "ada_asf";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/asf");
