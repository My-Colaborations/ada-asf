<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">

  <managed-bean>
    <managed-bean-name>sessionForm</managed-bean-name>
    <managed-bean-class>ASF.Applications.Tests.Form_Bean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>minimum</property-name>
      <property-class>int</property-class>
      <value>1</value>
    </managed-property>
    <managed-property>
      <property-name>maximum</property-name>
      <property-class>int</property-class>
      <value>10</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>requestForm</managed-bean-name>
    <managed-bean-class>ASF.Applications.Tests.Form_Bean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>applicationForm</managed-bean-name>
    <managed-bean-class>ASF.Applications.Tests.Form_Bean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  
</faces-config>
