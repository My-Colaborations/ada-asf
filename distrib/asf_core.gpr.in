with "util";
with "util_config";
with "el";
with "security";

library project Asf_Core is
   Version := "@ASF_VERSION@";

   for Library_Name use "ada_asf";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/ada-asf");
   for Library_ALI_Dir use Util_Config.Alidir & "/ada-asf/";
   for Externally_Built use "true";
end Asf_Core;
