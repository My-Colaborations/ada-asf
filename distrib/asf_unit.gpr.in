with "util";
with "el";
with "asf_core";
with "util_unit";
with "util_config";

library project Asf_Unit is
   Version := "@ASF_VERSION@";

   for Library_Name use "ada_asf_unit";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/ada-asf-unit");
   for Library_ALI_Dir use Util_Config.Alidir & "/ada-asf-unit/";
   for Externally_Built use "true";
end Asf_Unit;

