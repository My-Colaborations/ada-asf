with "util";
with "util_http_aws";
with "util_config";
with "el";
with "security";
with "asf_core";
@WITH_SERVER@

library project Asf_Aws is
   Version := Asf_Core.Version;

   for Library_Name use "ada_asf_aws";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/ada-asf-aws");
   for Library_ALI_Dir use Util_Config.Alidir & "/ada-asf-aws/";
   for Externally_Built use "true";
end Asf_Aws;
